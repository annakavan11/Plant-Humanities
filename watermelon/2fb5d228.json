      "body": [
        {
          "type": "TextualBody",
          "value": "Various methods of preserving watermelons from colored ravages have been tried...",
          "purpose": "commenting"
        }
      ],
      "target": {
        "selector": {
          "type": "FragmentSelector",
          "conformsTo": "http://www.w3.org/TR/media-frags/",
